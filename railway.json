{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "RAILPACK"
  },
  "deploy": {
    "runtime": "V2",
    "numReplicas": 1,
    "preDeployCommand": [
      "python manage.py migrate --noinput",
      "python manage.py collectstatic --noinput"
    ],
    "sleepApplication": false,
    "useLegacyStacker": false
  }
}
